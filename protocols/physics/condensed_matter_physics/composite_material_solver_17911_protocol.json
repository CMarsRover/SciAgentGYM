                        },
                        "description": "各组件的横截面积，单位为 mm²"
                    },
                    "elastic_moduli": {
                        "type": "array",
                        "items": {
                            "type": "number"
                        },
                        "description": "各组件的弹性模量，单位为 N/mm²"
                    },
                    "lengths": {
                        "type": "array",
                        "items": {
                            "type": "number"
                        },
                        "description": "各组件的长度，单位为 mm"
                    },
                    "max_stresses": {
                        "type": "array",
                        "items": {
                            "type": "number"
                        },
                        "description": "各组件的最大允许应力，单位为 N/mm²"
                    }
                },
                "required": [
                    "areas",
                    "elastic_moduli",
                    "lengths",
                    "max_stresses"
                ]
            }
        },
        "additionalProperties": {
            "function_path": "./tools/composite_material_solver_17911.py"
        }
    },
